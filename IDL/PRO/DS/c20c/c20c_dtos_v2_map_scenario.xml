    CAM4-2degree
  </attribute_model_id>
  <attribute_experiment_family_experiment_subexperiment>
    All-Hist_est1_v1-0
  </attribute_experiment_family_experiment_subexperiment>
  <tossic_source>
    NOAA-OI-v2
  </tossic_source>
